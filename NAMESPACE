# mchi
export(mchi)
S3method(print, mchi)
# support_funcs
export(cm2in, make_key, se, rnt, theme_takiwaR)
# predicates
exportPattern("^has_*", "^is_*", "^all_*")
# read
export(read_takRexcel)
export(new_read_definition)
export(takRblock)

# print methods for core types
S3method(print, takRcollection)
S3method(print, takRempty)
S3method(print, takRmeta)
S3method(print, takRrow)
S3method(print, takRperc)
S3method(print, takRcount)
S3method(print, takRsf)
S3method(print, takRwide)
S3method(print, takRdef)

# validate.R
export(make_validator, validate, negate)
export(validators, "validators<-", get_default_validators)
export(predicates, "predicates<-")
S3method(print, takRpredicate)
S3method(print, takRpredicates)
S3method(validate, takRcollection)
S3method(validate, takRdef)
S3method(validate, default)


S3method(str, takRdef)
S3method(str, takRblock)
S3method(str, takRpredicate)
S3method(str, takRpredicates)
S3method(str, takRvalidator)
S3method(str, takRvalidators)

